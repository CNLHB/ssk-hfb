<view class="body"><input class="uni-input common-input" type="text" placeholder="输入需要绑定的邮箱" data-event-opts="{{[['input',[['__set_model',['','email','$event',[]]]]]]}}" value="{{email}}" bindinput="__e"/><view class="login-input-box"><input class="uni-input common-input forget-input" type="text" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getCheckNum',['$event']]]]]}}" class="forget u-f-ajc login-font-color yanzhengma" bindtap="__e"><view class="u-f-ajc">{{!codetime?'获取验证码':codetime+' s'}}</view></view></view><button class="{{['user-set-btn',(disabled)?'user-set-btn-disable':'']}}" loading="{{loading}}" type="primary" disabled="{{disabled}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">完成</button></view>